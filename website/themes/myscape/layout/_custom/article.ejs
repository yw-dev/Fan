<article id="<%= post.layout %>-<%= post.slug %>" class="article article-type-<%= post.layout %>" itemscope itemprop="blogPost">
  <div class="article-meta">
    <%- partial('post/category') %>
  </div>
  <div class="article-inner">
    <%# -Carousel Panel- %>
    <% if (post.layout == 'categories') { %>
        <%- partial('_custom/carousel', {Carousel:[theme.Carousel_Work,theme.Carousel_Design,theme.Carousel_Learn]}, {cache: !config.relative_link}) %>
    <% } else if (post.layout == 'home'){ %>
        <% if (theme.Carousel_Work.enabled) { %>
            <%- partial('_custom/carousel', {Carousel:[theme.Carousel_Work]}, {cache: !config.relative_link}) %>
        <% } %>
    <% } else if (post.layout == 'worksite'){ %>
        <% if (theme.Carousel_Learn.enabled) { %>
            <%- partial('_custom/carousel', {Carousel:[theme.Carousel_Work]}, {cache: !config.relative_link}) %>
        <% } %>
    <% } else if (post.layout == 'designsite'){ %>
        <% if (theme.Carousel_Learn.enabled) { %>
            <%- partial('_custom/carousel', {Carousel:[theme.Carousel_Design]}, {cache: !config.relative_link}) %>
        <% } %>
    <% } else if (post.layout == 'learnsite'){ %>
        <% if (theme.Carousel_Learn.enabled) { %>
            <%- partial('_custom/carousel', {Carousel:[theme.Carousel_Learn]}, {cache: !config.relative_link}) %>
        <% } %>
    <% } else if (post.layout == 'statistical'){ %>
        <% if (theme.Carousel_Learn.enabled) { %>
            <%- partial('_custom/carousel', {Carousel:[theme.Carousel_Learn]}, {cache: !config.relative_link}) %>
        <% } %>
    <% } else if (post.layout == 'gamesite'){ %>
        <% if (theme.Carousel_Learn.enabled) { %>
            <%- partial('_custom/carousel', {Carousel:[theme.Carousel_Game]}, {cache: !config.relative_link}) %>
        <% } %>
    <% } else if (post.layout == 'videosite'){ %>
        <% if (theme.Carousel_Learn.enabled) { %>
            <%- partial('_custom/carousel', {Carousel:[theme.Carousel_Video]}, {cache: !config.relative_link}) %>
        <% } %>
    <% } %>
    <%# -Carousel Panel End- %>
    <div class="article-entry" itemprop="articleBody">
    
  </div>
</article>
