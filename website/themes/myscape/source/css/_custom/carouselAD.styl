*   CSS hacks
*****************/
@media (min-width: 480px){
    .Pane-container{
        padding-left: 30px;
        padding-right: 30px;
    }
}

@media (min-width: 768px){
    .Pane-container{
        padding-left: 40px;
        padding-right: 40px;
    }
}

@media (min-width: 992px){
    .Pane--adaptiveBeforeSpace{
        padding-top: 40px;
    }
    .Pane--adaptiveAfterSpace{
        padding-bottom: 40px;
    }
}